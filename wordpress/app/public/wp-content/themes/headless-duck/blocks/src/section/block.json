{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tec/section",
	"version": "0.1.0",
	"title": "Panel Section",
	"category": "duck-sections",
	"icon": "align-left",
	"description": "Basic custom section",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Headline Here"
		},
		"subtitle": {
			"type": "string"
		},
		"reverseAlignment": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
